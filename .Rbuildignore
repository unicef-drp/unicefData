# R package build ignores
^unicefData\.Rproj$
^\.Rproj\.user$
^\.Rhistory$
^\.RData$
^\.Rdata$

# Git
^\.git$
^\.gitignore$

# Python package (separate from R package)
^python$
^python/.*$

# Examples folder (not part of R package)
^R/examples$
^R/examples/.*$

# Documentation
^README\.Rmd$
^README\.md$
^NEWS\.md$
^LICENSE\.md$
^docs$
^_pkgdown\.yml$

# CI/CD
^\.github$
^\.travis\.yml$
^appveyor\.yml$
^codecov\.yml$

# Logs and temp files
^log$
^logs$
^testthat$
^data$

# IDE
^\.vscode$
^\.idea$

^R/README\.md$
^R/tests/output/\.gitkeep$

# Non-standard directories for multi-language repo
^config$
^metadata$
^stata$
^stataonly$
^validation$
^internal$
^paper$
^benchmarks$
^drafts$

# Stata files
^.*\.do$

# TODO/development files
^TODO.*\.md$

# Hidden files (.gitkeep)
.*\.gitkeep$

# Python artifacts
^\.pytest_cache$

# Log files and temp outputs
^check_output\.txt$
^stata\.log$
^test_multi_indicator\.log$
^.*\.log$
