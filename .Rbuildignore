# R package build ignores
^unicefData\.Rproj$
^\.Rproj\.user$
^\.Rhistory$
^\.RData$
^\.Rdata$

# Git
^\.git$
^\.gitignore$

# Python package (separate from R package)
^python$
^python/.*$

# Examples folder (not part of R package)
^R/examples$
^R/examples/.*$

# Documentation
^README\.Rmd$
^README\.md$
^NEWS\.md$
^LICENSE\.md$
^docs$
^_pkgdown\.yml$

# CI/CD
^\.github$
^\.travis\.yml$
^appveyor\.yml$
^codecov\.yml$

# Logs and temp files
^log$
^logs$
^testthat$
^data$

# IDE
^\.vscode$
^\.idea$

^R/README\.md$
^R/tests/output/\.gitkeep$

# Non-standard directories for multi-language repo
^config$
^metadata$
^stata$
^stataonly$
^validation$
^internal$
^paper$
^benchmarks$
^drafts$

# Stata files
^.*\.do$

# TODO/development files
^TODO.*\.md$

# Hidden files (.gitkeep)
.*\.gitkeep$

# Python artifacts
^\.pytest_cache$

# Log files and temp outputs
^check_output\.txt$
^check_full_output\.txt$
^check_no_vignettes\.txt$
^build_output\.txt$
^install_deps\.R$
^install_all_deps\.R$
^run_check\.R$
^stata\.log$
^test_multi_indicator\.log$
^.*\.log$

# Non-standard top-level files
^cran-comments\.md$
^CHANGELOG\.md$
^CITATION\.cff$
^install_package\.R$
^scripts$
^CONTRIBUTING\.md$
^CRAN_SUBMISSION_GUIDE\.md$
^build_vignettes\.R$
^build_vignettes_manual\.R$
^build_vignettes_rstudio\.R$
^cran_submission_final\.R$
^final_check_output\.txt$
^final_cran_check\.R$
^final_cran_check_output\.txt$
^install_pandoc\.R$
^install_pandoc\.bat$
^setup_pandoc_and_build\.ps1$
^vignette_footer_template\.md$

# Non-standard R/ subdirectories (CRAN compliance)
^R/tests$
^R/tests/.*$
^R/scripts$
^R/scripts/.*$
^CRAN-SUBMISSION$
^nul$
